apiVersion: apps/v1
kind: Deployment
metadata:
  labels:
    app.kubernetes.io/app: notary
    app.kubernetes.io/component: notary
    app.kubernetes.io/part-of: cryptosoccer
    app.kubernetes.io/version: 1.0.0
  name: notary
spec:
  replicas: 1
  selector:
    matchLabels:
      app.kubernetes.io/app: notary
      app.kubernetes.io/component: notary
      app.kubernetes.io/part-of: cryptosoccer
      app.kubernetes.io/version: 1.0.0
  template:
    metadata:
      labels:
        app.kubernetes.io/app: notary
        app.kubernetes.io/component: notary
        app.kubernetes.io/part-of: cryptosoccer
        app.kubernetes.io/version: 1.0.0
    spec:
      containers:
      - args:
        - -postgres
        - postgres://$(freeverse_username):$(freeverse_password)@marketdb:5432/market?sslmode=disable
        - -ethereum
        - http://xdai:8545
        - -private_key
        - $(notary_private_key)
        - -market_address
        - $(MARKET_CONTRACT_ADDRESS)
        - -constantsgetters_address
        - $(CONSTANTSGETTERS_CONTRACT_ADDRESS)
        - -privileged_address
        - $(PRIVILEGED_CONTRACT_ADDRESS)
        - -market_id
        - $(market_id)
        - -google_key
        - /var/secrets/google/key.json
        envFrom:
        - configMapRef:
            name: freeverse-configmap
        image: freeverseio/market.notary:dev
        imagePullPolicy: Always
        name: notary
        ports:
        - containerPort: 4000
        volumeMounts:
        - mountPath: /var/secrets/google
          name: google-cloud-key
      imagePullSecrets:
      - name: docker-secret
      volumes:
      - name: google-cloud-key
        secret:
          secretName: google-iap-key
---
