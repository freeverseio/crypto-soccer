            name: freeverse-configmap
        image: freeverseio/market.notary:0.18.1
        imagePullPolicy: Always
        name: notary
        ports:
        - containerPort: 4000
        volumeMounts:
        - mountPath: /var/secrets/google
          name: google-cloud-key
      imagePullSecrets:
